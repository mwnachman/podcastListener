{
  parser: "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "no-unused-vars": 0,
    "quotes": [
      "error",
      "single"
    ],
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "react/prop-types": [2],
    "react/jsx-no-duplicate-props": [
      "error",
      { "ignoreCase": true }
    ],
    "react/no-unused-prop-types": [
      "error"
    ],
    "react/prefer-stateless-function": [
      "error"
    ],
    "no-console": [
      "error",
      { "allow": [
          "warn",
          "error"
        ]
      }
    ]
  },
  "settings": {
    "react": {
      "version": "16.5.2"
    }
  }
}